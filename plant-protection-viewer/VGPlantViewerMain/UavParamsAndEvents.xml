    <ParameterLimit type="float" id="MPC_XY_CRUISE" name="作业速度" group="undef" least="1" biggest="7"/>
    <ParameterLimit type="float" id="MIS_AB_TF_ALT" name="AB点仿地高度" group="undef" least="0.5" biggest="10" />
